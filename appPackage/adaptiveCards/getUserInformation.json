{
    "type": "AdaptiveCard",
    "$schema": "https://adaptivecards.io/schemas/adaptive-card.json",
    "version": "1.5",
    "body": [
        {
            "type": "TextBlock",
            "text": "@odata.context: ${if(@odata.context, @odata.context, 'N/A')}",
            "wrap": true
        },
        {
            "type": "Container",
            "$data": "${value}",
            "items": [
                {
                    "type": "TextBlock",
                    "text": "value.id: ${if(id, id, 'N/A')}",
                    "wrap": true
                },
                {
                    "type": "TextBlock",
                    "text": "value.createdDateTime: ${if(createdDateTime, createdDateTime, 'N/A')}",
                    "wrap": true
                },
                {
                    "type": "TextBlock",
                    "text": "value.lastModifiedDateTime: ${if(lastModifiedDateTime, lastModifiedDateTime, 'N/A')}",
                    "wrap": true
                },
                {
                    "type": "TextBlock",
                    "text": "value.webUrl: ${if(webUrl, webUrl, 'N/A')}",
                    "wrap": true
                }
            ]
        }
    ]
}
